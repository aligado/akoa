<% include newheader %>
<div class="container">
  <div class="ui form segment" style="width:1000px;margin:40px auto">
  <style>
    .ui.padded.table th, .ui.padded.table td { padding: 0.2em 0.2em; }
  </style>
  <table class="ui celled padded table">
    <thead>
      <tr>
        <% ths.forEach( function(item, index) { %>
          <th>
            <%= item %>
          </th>
        <% }) %>
      </tr>
    </thead>
    <tbody>
      <% tds.forEach( function(td, index) { %>
        <tr>
          <% td.forEach( function(v, k) { %>
            <td><%- v %></td>
          <% }) %>
        </tr>
      <% }) %>
    </tbody>
  </table>
  </div>
</div>
<script type="text/javascript">
  $('.ui.dropdown').dropdown();
</script>

<% include footer %>